{
  "name": "full-stack-coding-exercise",
  "version": "1.0.0",
  "description": "full stack exercise for gstv",
  "main": "index.js",
  "scripts": {
    "install-client-dep": "cd ./client && npm install",
    "install-api-dep": "cd ./api && npm install",
    "install-dep": "npm run install-api-dep && npm run install-client-dep",
    "start-api": "cd ./api && npm run start",
    "start-client": "cd ./client && npm run start",
    "start": "npm run start-api && npm run start-client",
    "stop-api": "cd ./api && npm run stop",
    "test-client": "cd ./client && npm run test",
    "test-api": "cd ./api && npm run test",
    "test": "npm run test-client && npm run test-api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphaXkiller/full-stack-coding-exercise.git"
